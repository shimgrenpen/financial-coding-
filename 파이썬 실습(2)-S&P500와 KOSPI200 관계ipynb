    "#날짜 형식으로 바꾸는 함수 정의\n",
    "def date_format(d):\n",
    "    d=str(d).replace('-','.') #날짜 구분자가 .이 아닌 -으로 되어 있는 경우, .으로 변환해주는 기능을 한다.\n",
    "    yyyy=int(d.split('.')[0])\n",
    "    mm=int(d.split('.')[1])\n",
    "    dd=int(d.split('.')[2])\n",
    "    \n",
    "    this_date=dt.date(yyyy,mm,dd)\n",
    "    return this_date"
   ]
  },
